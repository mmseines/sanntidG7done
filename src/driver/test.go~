package main

import (
    "driver" 
    "fmt"
    "time"
)

func main(){
    fmt.Printf("du st√•r i: ", driver.GetFloor(), " etasje")
    driver.SetFloorIndicator(driver.GetFloor())
    driver.SetSpeed(20)
    time.Sleep(1*time.Second)
    driver.SetSpeed(0)
    driver.SetLamp("down", 1, 1)
    driver.SetLamp("down", 0, 1)
    driver.SetOpenDoor(1)

}

